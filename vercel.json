{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/@prisma/client/schema.prisma",
          "node_modules/.prisma/client/query-engine-darwin-arm64",
          "node_modules/.prisma/client/query-engine-linux-glibc-libssl1.1.0"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}